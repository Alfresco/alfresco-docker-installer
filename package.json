{
  "name": "generator-alfresco-docker-installer",
  "version": "0.1.7",
  "description": "Alfresco Docker Installer",
  "homepage": "https://github.com/Alfresco/alfresco-docker-installer",
  "author": "Angel Borroy <angel.borroy@alfresco.com>",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "alfresco",
    "docker-compose",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "wait-on": "^4.0.1",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfresco/alfresco-docker-installer.git"
  },
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/alfresco/alfresco-docker-installer/issues"
  }
